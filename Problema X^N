#include <stdio.h>
int main ()
{ 
    int x;
    int temp;
    scanf("%i",&x);
    
    int n;
    scanf("%i",&n);
    int y = x;
    int contador;
 
 do {
    y = y*x;
    temp = y;
    y = x;
    x = temp;
    
    contador = contador +1;
 }    
    while (contador != n);
    
    printf("%i", x);
    
    return 0;
}
