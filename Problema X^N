#include <stdio.h>
int main ()
{ 
    int x;
    
    scanf("%i",&x);
    
    int n;
    scanf("%d",&n);
    
    int contador;
    int temp;
    if (n == 0)
    {
        printf("1");
    }
    
    else {
    do {
   
   // multiplicar x por x e guardar valor e multiplicar de novo se contador n√£o tiver chegado a n
    
    contador = contador +1;
    }
    
    while (contador <= n);
    
    printf("%i", x);
    return 0;
    };
}
