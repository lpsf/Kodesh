#include <stdio.h>
int main()
{
    int m, n, x, y, max, lim;
    scanf("%d %d", &n, &m);
    lim = 1;
    
    while(max >= lim)
    {
    for(y = 0; y <= n; y++)
    {
        for(x = 0; x <= m; x++)
        {
            lim = max;
            max = (x*y) - (x*x) + y;
            
        };
    };
};
    
    printf("%d x=%d y=%d", max, x, y);
    return 0;
}
