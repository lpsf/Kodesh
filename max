#include <stdio.h>
int main()
{
    int m, n, x, y, f, teste;
    scanf("%d %d", &m, &n);
    teste = x + y + f;
    testef = 0;
  
  while (testef >= teste)
  {
        testef = teste
        for(x = 0; x <= m; x++)
        {
            for(y = 0; y <= n; y++)
            {
                
              
                f = (x*y) - (x*x) + y;
                printf("x = %i y = %i, f = %i\n", x, y, f); 
                 
            }
        } 
       
	}
    printf("%d x=%d y=%d", testef, x, y);
    return 0;
}
