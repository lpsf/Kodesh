#include <stdio.h>
int main()
{
    int m, n, x, y, max, f;
    scanf("%d %d", &n, &m);
    max = 0;
    
   do
   {
    for(x = 0; x <= m; x++)
    {
        for(y = 0; y <= n; y++)
        {
            max = f;
            f = (x*y) - (x*x) + y;
            
        };
    };
   }
   while(max <= f);

    
    printf("%d x=%d y=%d", f, x, y);
    return 0;
}
